{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://playingarts.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://playingarts.com http://localhost:3000"
        }
      ]
    }
  ]
}
