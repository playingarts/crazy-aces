# EmailJS Configuration
# Get these values from https://dashboard.emailjs.com/
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Email Templates
# Template IDs for different discount tiers
VITE_EMAILJS_TEMPLATE_5=your_template_id_for_5_percent
VITE_EMAILJS_TEMPLATE_10=your_template_id_for_10_percent
VITE_EMAILJS_TEMPLATE_15=your_template_id_for_15_percent

# Discount Codes
# NOTE: In production, these should be generated and validated server-side
# These are temporary client-side codes for the MVP
VITE_DISCOUNT_CODE_5=your_5_percent_code
VITE_DISCOUNT_CODE_10=your_10_percent_code
VITE_DISCOUNT_CODE_15=your_15_percent_code
